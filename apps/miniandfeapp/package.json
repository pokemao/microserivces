{
  "name": "@syz/microservices-miniandfeapp",
  "version": "1.0.0",
  "private": true,
  "description": "no",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Less",
    "framework": "React"
  },
  "scripts": {
    "start": "node ./static-server/index.cjs",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5 --env-prefix MICRO_APP_",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch --env-prefix MICRO_APP_",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch --env-prefix MICRO_APP_",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch --env-prefix MICRO_APP_",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "serve": "pnpm serve:h5",
    "build": "pnpm build:h5 && tsx ./static-server/compiler/esbuild.config.ts",
    "docker-build": "cross-env DOCKER_BUILDKIT=0 COMPOSE_DOCKER_CLI_BUILD=0 tsx ./scripts/docker/ts/genImage.ts",
    "docker-push": "tsx ./scripts/docker/ts/pushImage.ts",
    "k8s-genyaml": "tsx ./scripts/k8s/ts/JSON2YAML.ts",
    "k8s-justgenyaml": "tsx ./scripts/k8s/ts/JSON2YAML.ts",
    "k8s-applyOrRolloutRestart": "tsx ./scripts/k8s/ts/applyOrRolloutRestart.ts"
  },
  "browserslist": {
    "development": [
      "defaults and fully supports es6-module",
      "maintained node versions"
    ],
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8"
    ]
  },
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@nutui/nutui-react-taro": "^2.7.8",
    "@tarojs/components": "4.0.9",
    "@tarojs/helper": "4.0.9",
    "@tarojs/plugin-framework-react": "4.0.9",
    "@tarojs/plugin-html": "4.0.9",
    "@tarojs/plugin-http": "^4.0.9",
    "@tarojs/plugin-platform-alipay": "4.0.9",
    "@tarojs/plugin-platform-h5": "4.0.9",
    "@tarojs/plugin-platform-harmony-hybrid": "4.0.9",
    "@tarojs/plugin-platform-jd": "4.0.9",
    "@tarojs/plugin-platform-qq": "4.0.9",
    "@tarojs/plugin-platform-swan": "4.0.9",
    "@tarojs/plugin-platform-tt": "4.0.9",
    "@tarojs/plugin-platform-weapp": "4.0.9",
    "@tarojs/react": "4.0.9",
    "@tarojs/runtime": "4.0.9",
    "@tarojs/shared": "4.0.9",
    "@tarojs/taro": "4.0.9",
    "ms": "^2.1.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-react": "^7.24.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@syz/microservices-typescript-config": "workspace:^",
    "@tarojs/cli": "4.0.9",
    "@tarojs/taro-loader": "4.0.9",
    "@tarojs/webpack5-runner": "4.0.9",
    "@types/node": "^18",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "4.0.9",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.0.9",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "less": "^4.2.0",
    "postcss": "^8.4.38",
    "react-refresh": "^0.14.0",
    "stylelint": "^16.4.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "5.91.0"
  }
}
